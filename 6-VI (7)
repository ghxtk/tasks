#include <iostream>
using namespace std;

int main() {
	int n, m; cin >> n >> m;
	int** a = new int* [n];
	for (int i = 0; i < n; i++)
		a[i] = new int[m];

	for (int i = 0; i < n; i++) {
		for (int j = 0; j < m; j++) {
			cin >> a[i][j];
		}
	}

	for (int j = 0; j < m; j++) {
		if (a[0][j] > a[n-1][j]){
			for (int z = j; z < m - 1; z++) {
				for (int i = 0; i < n; i++) {
					a[i][z] = a[i][z + 1];
				}
			}
			m--;
			j--;
		}
	}

	for (int i = 0; i < n; i++, cout << endl)
		for (int j = 0; j < m; j++)
			cout << a[i][j]<<" ";
	for (int i = 0; i < n; i++)
		delete[] a[i];
	delete[] a;
	return 0;
}
