#include <iostream>
using namespace std;

int main() {
	int n; cin >> n;
	int** a = new int *[n];
	for (int i = 0; i < n; i++)
		a[i] = new int[n];
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			cin >> a[i][j];
		}
	}
  
	for (int j = 0; j < n; j++) {
		int m = 0;
		for (int i = 0; i < n; i++) {
			if (a[m][j] > a[i][j]) {
				m = i;
			}
		}
		a[m][j] = 0;
	}
  
	cout << endl;
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			cout << a[i][j]<<" ";
		}
		cout << endl;
	}
	return 0;
}
