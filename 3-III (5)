#include <iostream>
using namespace std;

int main() {
	int i = 1;
	while (i <= 10) {
		cout << i << " " << i * 0.453 << endl;
		++i;
	}
	cout << endl;

	int j = 1;
	do {
		cout << j << " " << j * 0.453 << endl;
		j++;
	} while (j <= 10);
	cout << endl;

	for (int i = 1; i <= 10; i++) {
		cout << i << " " << i * 0.453 << endl;
	}
	return 0;
}
